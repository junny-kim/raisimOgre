<robot name="2DrobotArm">
    <link name="world"/>

    <link name="link1">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="1"/>
            <inertia ixx="0.002" ixy="0.0" ixz="0" iyy="0.002" iyz="0" izz="0.002"/>
        </inertial>
    </link>

    <joint name="worldTolink1" type="fixed">
        <parent link="world"/>
        <child link="link1"/>
        <origin xyz="0 0 10"/>
    </joint>

    <link name="link2">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="1"/>
            <inertia ixx="0.002" ixy="0.0" ixz="0" iyy="0.002" iyz="0" izz="0.002"/>
        </inertial>
    </link>

    <joint name="link1Tolink2" type="spherical">
        <parent link="link1"/>
        <child link="link2"/>
        <origin xyz="0 0 -0.50"/>
        <axis xyz="0 1 0"/>
        <limit lower="1"/>
    </joint>



</robot>
